CREATE TABLE employees1 (
    ename VARCHAR(50),
    dept_name VARCHAR(50),
    designation VARCHAR(50),
    salary INT,
    date_of_join DATE
);
INSERT INTO employees1(ename, dept_name, designation, salary, date_of_join)
